\x91\xe9\xa2\x9d\xef\xbc\x8c\xe6\xb3\xa8\xe6\x84\x8f\xe6\x98\xaf\xe4\xbb\xa5\xe4\xb8\x87\xe4\xb8\xba\xe5\x8d\x95\xe4\xbd\x8d\xef\xbc\x8c\xe5\x8d\x95\xe7\xac\x94\xe9\x87\x91\xe9\xa2\x9d\xe4\xb8\x8d\xe8\x83\xbd\xe8\xb6\x85\xe8\xbf\x8710000\xe4\xb8\x87\');\r\n\t\t\treturn false;\r\n\t\t}  \r\n\t\tvar isMobile = /^1[0-9]{10}$/;\r\n\t\tif(f.exttp.value=="") {   \r\n\t\t\talert("\xe5\xaf\xb9\xe4\xb8\x8d\xe8\xb5\xb7\xef\xbc\x8c\xe8\xaf\xb7\xe8\xbe\x93\xe5\x85\xa5\xe8\x81\x94\xe7\xb3\xbb\xe7\x94\xb5\xe8\xaf\x9d[\xe6\x89\x8b\xe6\x9c\xba\xe5\x8f\xb7\xe7\xa0\x81]!");\r\n\t\t\treturn false;   \r\n\t\t}    \r\n\t\tif(!isMobile.test(f.exttp.value)){   \r\n\t\t\talert("\xe5\xaf\xb9\xe4\xb8\x8d\xe8\xb5\xb7\xef\xbc\x8c\xe8\xaf\xb7\xe8\xbe\x93\xe5\x85\xa5\xe6\xad\xa3\xe7\xa1\xae\xe7\x9a\x84\xe6\x89\x8b\xe6\x9c\xba\xe5\x8f\xb7\xe7\xa0\x81;\xe4\xbe\x8b\xe5\xa6\x82:13916752109");\r\n\t\t\treturn false; \r\n\t\t}\r\n\t\tif(!confirm("\xe6\xb8\xa9\xe9\xa6\xa8\xe6\x8f\x90\xe7\xa4\xba\xef\xbc\x9a\xe6\x82\xa8\xe7\x9a\x84\xe4\xbf\xa1\xe6\x81\xaf\xe4\xb8\x80\xe7\xbb\x8f\xe5\x8f\x91\xe5\xb8\x83\xef\xbc\x8c\xe5\xb0\x86\xe4\xbc\x9a\xe6\x9c\x89\xe8\xb4\xb7\xe6\xac\xbe\xe6\x9c\xba\xe6\x9e\x84\xe4\xb8\xbb\xe5\x8a\xa8\xe4\xb8\x8e\xe6\x82\xa8\xe8\x81\x94\xe7\xb3\xbb\xe3\x80\x82\xe8\x8e\xb7\xe8\xb4\xb7\xe5\x89\x8d\xef\xbc\x8c\xe8\xaf\xb7\xe5\x8b\xbf\xe6\x94\xaf\xe4\xbb\x98\xe4\xbb\xbb\xe4\xbd\x95\xe8\xb4\xb9\xe7\x94\xa8\xef\xbc\x8c\xe4\xbb\xa5\xe5\x85\x8d\xe4\xb8\x8a\xe5\xbd\x93\xe5\x8f\x97\xe9\xaa\x97\xef\xbc\x81\xe7\x82\xb9\xe5\x87\xbb\xe2\x80\x98\xe7\xa1\xae\xe5\xae\x9a\xe2\x80\x99\xe6\x8f\x90\xe4\xba\xa4")) {\r\n\t\t\treturn false;\r\n\t\t}\r\n\t\treturn true;\r\n    }\r\n</script>\r\n\r\n\r\n<div class="online_apply">\r\n    <form action="/shenqing/app/" method="post" onsubmit="return check_form(this);">\r\n\t<div class="label1"><span>\xe8\xb4\xb7\xe6\xac\xbe\xe7\xb1\xbb\xe5\x9e\x8b\xef\xbc\x9a</span><input type="radio" name="usertype" value="0" checked="checked" />\xe4\xb8\xaa\xe4\xba\xba\xe8\xb4\xb7\xe6\xac\xbe<input type="radio" name="usertype" value="1" />\xe4\xbc\x81\xe4\xb8\x9a\xe8\xb4\xb7\xe6\xac\xbe</div>\r\n\r\n\t<div class="label2">\r\n\t    <span>\xe5\xa7\x93&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\xe6\xb0\x8f\xef\xbc\x9a</span>\r\n\t    <input name="name" type="text" size="4" maxlength="4" />\r\n\t    <select name="sex" id="sex">\r\n\t\t<option selected="selected" value="1">\xe5\x85\x88\xe7\x94\x9f</option>\r\n\t\t<option value="2">\xe5\xa5\xb3\xe5\xa3\xab</option>\r\n\t    </select> \r\n\t</div>\r\n\r\n\t<div class="label3">\r\n\t    <span>\xe6\x9c\x89\xe6\x97\xa0\xe6\x8a\xb5\xe6\x8a\xbc\xef\xbc\x9a</span>\r\n\t    <input type="radio" name="classid" value="2" checked="checked" />\r\n\t    \xe6\x9c\x89\xe6\x8a\xb5\xe6\x8a\xbc&nbsp;&nbsp;\r\n\t    <input type="radio" name="classid" value="3" />\r\n\t    \xe6\x97\xa0\xe6\x8a\xb5\xe6\x8a\xbc \r\n\t</div>\r\n\r\n\t<div class="label4">\r\n\t    <span>\xe8\xb4\xb7\xe6\xac\xbe\xe9\x87\x91\xe9\xa2\x9d\xef\xbc\x9a</span>\r\n\t    <input name="money" type="text" size="6" maxlength="6" />\r\n\t    &nbsp;&nbsp;\xe4\xb8\x87\xe5\x85\x83\r\n\t</div> \r\n\r\n\t<div class="label5">\r\n\t    <span>\xe8\x81\x94\xe7\xb3\xbb\xe6\x89\x8b\xe6\x9c\xba\xef\xbc\x9a</span>\r\n\t    <input name="exttp" type="text" size="11" maxlength="11" /> \r\n\t</div>\r\n\r\n\t<div class="label_but">\r\n\t\t<input type="hidden" name=\'storeId\' value="53">\r\n\t    <input type="submit" value=" " name="ksformsubmit_i" class="label_butzx" onmouseover="this.className=\'label_butzx2\'" onmouseout="this.className=\'label_butzx\'">\r\n\t</div>\r\n\r\n    </form> \r\n</div>\r\n<div class="clear"></div>\r\n\r\n\r\n\r\n      </div>\n      <div class="main_bottom">\n        <div class="main_bleft"></div>\n        <div class="main_bcenter mb_normal"></div>\n        <div class="main_bright"></div>\n      </div>\n    </li>\n        <li class="widget style_gray" id="contact">\n      <div class="main_top">\n        <div class="main_tleft"></div>\n        <div class="main_tcenter mt_normal">\n          <h3 class="hei14">\n            \xe8\x81\x94\xe7\xb3\xbb\xe6\x96\xb9\xe5\xbc\x8f          </h3>\n          \t\t  <a href="/lists/index/contact_storeId-53/" target="_blank" title="\xe6\x9b\xb4\xe5\xa4\x9a" class="more" ></a>\n                    <div class="widget1 widget-head"></div>\n        </div>\n        <div class="main_tright"></div>\n        <div class="clear"></div>\n      </div>\n      <div class="column_main cm_normal widget-content">\n        \xef\xbb\xbf        <div class="c_contact">\r\n        <ul>\r\n        <li>\xe7\x9c\x9f\xe5\xae\x9e\xe5\xa7\x93\xe5\x90\x8d\xef\xbc\x9a\xe6\xb2\xa1\xe6\x9c\x89\xe4\xbf\xa1\xe6\x81\xaf</li>\r\n\t\t <li >\xe8\x81\x94\xe7\xb3\xbb\xe7\x94\xb5\xe8\xaf\x9d 15828119132</li> \t\t \r\n\t\t   <li>\xe7\x94\xb5\xe5\xad\x90\xe9\x82\xae\xe4\xbb\xb6\xef\xbc\x9a494028165@qq.com</li>\t\t \t        \r\n        </ul>\r\n\r\n        </div> \r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n      </div>\n      <div class="main_bottom">\n        <div class="main_bleft"></div>\n        <div class="main_bcenter mb_normal"></div>\n        <div class="main_bright"></div>\n      </div>\n    </li>\n        <li class="widget style_gray" id="announcement">\n      <div class="main_top">\n        <div class="main_tleft"></div>\n        <div class="main_tcenter mt_normal">\n          <h3 class="hei14">\n            \xe5\xba\x97\xe9\x93\xba\xe5\x85\xac\xe5\x91\x8a          </h3>\n                    <div class="widget1 widget-head"></div>\n        </div>\n        <div class="main_tright"></div>\n        <div class="clear"></div>\n      </div>\n      <div class="column_main cm_normal widget-content">\n        \r\n        <!--\xe6\xa8\xa1\xe5\x9d\x97\xe5\x86\x85\xe5\xae\xb9\xe5\x8c\xba\xe5\x9f\x9f-->\r\n        <div class="notice">\r\n          <div class="notice_font">\r\n\t\t     <div class=\'notice_font1\'><strong class=\'red12\'></strong> </div>             <div class="notice_font1"><span class="notice_font1