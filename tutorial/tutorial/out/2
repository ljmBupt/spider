    <input type="button"  class="faq_but" onclick="window.location.href=\'/lists/list2/qa_storeId-51/?title=\' + document.question.q.value" value="\xe6\x88\x91\xe8\xa6\x81\xe6\x8f\x90\xe9\x97\xae"/>\n        </form>\n        </div>\n             </div>\n     <script language="javascript">\n      function check_value(o){\n\t\t  var value_tmp = o.value;\n\t\t  var str = value_tmp.replace(/\\s*/g,"");\n\t\t  if( str == \'\' ){\n\t\t  \talert(\'\xe6\x8f\x90\xe4\xba\xa4\xe5\x86\x85\xe5\xae\xb9\xe4\xb8\x8d\xe5\x8f\xaf\xe4\xb8\xba\xe7\xa9\xba!\');\n\t\t\treturn false;\n\t\t  }\n\t\t  return true;\n\t }\n\tfunction soquestiona(){\n\t\t\tvar status=true;\t\n\t\t\tvar title=$("#q").val();\n\t\t\ttitle=title.replace(/</gi,\'\xe3\x80\x88\');\n\t\t\ttitle=title.replace(/>/gi,\'\xe3\x80\x89\');\n\t\t\ttitle=title.replace(/\'/gi,"\xe2\x80\x98");\n\t\t\ttitle=title.replace(/"/gi,\'\xe2\x80\x9d\');\n\t\t\ttitle=title.replace(/ /gi,\'\');\n\t\t\t\tif(title=="\xe8\xaf\xb7\xe8\xbe\x93\xe5\x85\xa5\xe5\x85\xb3\xe9\x94\xae\xe5\xad\x97" || title==""|| title=="\xe8\xaf\xb7\xe8\xbe\x93\xe5\x85\xa5\xe6\x82\xa8\xe7\x9a\x84\xe9\x97\xae\xe9\xa2\x98\xef\xbc\x8c\xe5\xb0\xbd\xe9\x87\x8f\xe8\xaf\xa6\xe7\xbb\x86\xef\xbc\x8c\xe4\xbb\xa5\xe4\xbe\xbf\xe5\xbe\x97\xe5\x88\xb0\xe6\x9b\xb4\xe8\xaf\xa6\xe7\xbb\x86\xef\xbc\x8c\xe6\x9b\xb4\xe4\xb8\x93\xe4\xb8\x9a\xe7\x9a\x84\xe8\xa7\xa3\xe7\xad\x94"){\n\t\t\t\t\talert("\xe8\xaf\xb7\xe8\xbe\x93\xe5\x85\xa5\xe5\x85\xb3\xe9\x94\xae\xe5\xad\x97"); return false;\n\t\t\t\t}\n\t\t\t\twindow.open(\'http://www.edai.com/wenda/so_\'+title);return;\n\t}\n    </script>      </div>\n      <div class="main_bottom">\n        <div class="main_bleft"></div>\n        <div class="main_bcenter mb_normal"></div>\n        <div class="main_bright"></div>\n      </div>\n    </li>\n    <div class="clear"></div>\n        <li class="widget style_gray" id="news">\n      <div class="main_top">\n        <div class="main_tleft"></div>\n        <div class="main_tcenter mt_normal">\n          <h3 class="hei14">\n            \xe6\x96\xb0\xe9\x97\xbb\xe8\xb5\x84\xe8\xae\xaf          </h3>\n                    <div class="widget1 widget-head"></div>\n        </div>\n        <div class="main_tright"></div>\n        <div class="clear"></div>\n      </div>\n      <div class="column_main cm_normal widget-content">\n        <div class="column_main_list">\r\n    <ul>\r\n       \t\t             <li style=" height:auto; background:none;">\r\n              <h3>\r\n               <a class="blue12" href="http://www.edai.net/qydk/102563.html" target="_blank">\r\n                                      <img src="/mall/Resource/images/news01.jpg">\r\n                                 </a>\r\n               <a class="blue12 text_title" href="http://www.edai.net/qydk/102563.html" target="_blank">\xe4\xbd\x9b\xe5\xb1\xb1\xe5\xb7\xa5\xe8\xa1\x8c\xe4\xbb\x8a\xe5\xb9\xb4\xe5\xb7\xb2\xe5\x8f\x91\xe6\x94\xbe\xe5\xb0\x8f\xe4\xbc\x81\xe4\xb8\x9a\xe8\xb4\xb7\xe6\xac\xbe\xe8\xbf\x9160\xe4\xba\xbf\xe5\x85\x83</a>\r\n               <span>\r\n                \xe6\x98\xa8\xe6\x97\xa5\xef\xbc\x8c\xe8\xae\xb0\xe8\x80\x85\xe4\xbb\x8e\xe4\xb8\xad\xe5\x9b\xbd\xe5\xb7\xa5\xe5\x95\x86\xe9\x93\xb6\xe8\xa1\x8c\xe4\xbd\x9b\xe5\xb1\xb1\xe5\x88\x86\xe8\xa1\x8c\xe4\xba\x86\xe8\xa7\xa3\xe5\x88\xb0\xef\xbc\x8c\xe8\xaf\xa5\xe5\x88\x86\xe8\xa1\x8c\xe6\x88\xaa\xe8\x87\xb34\xe6\x9c\x88\xe4\xbb\xbd\xe7\xb4\xaf\xe8\xae\xa1\xe5\x8f\x91\xe6\x94\xbe\xe5\xb0\x8f\xe4\xbc\x81\xe4\xb8\x9a\xe8\xb4\xb7\xe6\xac\xbe\xe8\xbf\x9160\xe4\xba\xbf\xe5\x85\x83\xef\xbc\x8c\xe8\xb4\xb7\xe6\xac\xbe\xe4\xbd\x99\xe9\xa2\x9d\xe8\xbe\x83\xe5\x8e\xbb\xe5\xb9\xb4\xe5\x90\x8c\xe6\x9c\x9f\xe5\xa2\x9e\xe9\x95\xbf13%\xef\xbc\x8c\xe5\xb0\x8f\xe5\xbe\xae\xe4\xbc\x81\xe4\xb8\x9a\xe6\x9c\x89\xe8\xb4\xb7\xe6\x88\xb7\xe6\x95\xb0\xe8\xbe\xbe1300\xe6\x88\xb7\xe3\x80\x82 \xe4\xbd\x9b\xe5\xb1\xb1\xe5\xb7\xa5\xe8\xa1\x8c\xe5\xb0\x8f\xe4\xbc\x81\xe4\xb8\x9a\xe9\x83\xa8\xe8\xb4\x9f\xe8\xb4\xa3\xe4\xba\xba\xe8\xa1\xa8\xe7\xa4\xba\xef\xbc\x8c...\r\n      