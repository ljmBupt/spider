quee(h, speed, delay, sid) {\r\nvar t = null;\r\nvar p = false;\r\nvar o = document.getElementById(sid);\r\no.innerHTML += o.innerHTML;\r\no.onmouseover = function() {p = true}\r\no.onmouseout = function() {p = false}\r\no.scrollTop = 0;\r\nfunction start() {\r\n    t = setInterval(scrolling, speed);\r\n    if(!p) {\r\no.scrollTop += 2;\r\n}\r\n}\r\nfunction scrolling() {\r\n    if(p) return;\r\nif(o.scrollTop % h != 0) {\r\n        o.scrollTop += 2;\r\n        if(o.scrollTop >= o.scrollHeight/2) o.scrollTop = 0;\r\n    } else {\r\n        clearInterval(t);\r\n        setTimeout(start, delay);\r\n    }\r\n}\r\nsetTimeout(start, delay);\r\n}\t\t\t\r\n\r\n</script>\r\n<script>startMarquee(250, 120, 0, \'gymes\');</script>\r\n\r\n      </div>\n      <div class="main_bottom">\n        <div class="main_bleft"></div>\n        <div class="main_bcenter mb_normal"></div>\n        <div class="main_bright"></div>\n      </div>\n    </li>\n    <div class="clear"></div>\n        <li class="widget style_blue" id="products">\n      <div class="main_top">\n        <div class="main_tleft"></div>\n        <div class="main_tcenter mt_normal">\n          <h3 class="hei14">\n            \xe8\xb4\xb7\xe6\xac\xbe\xe4\xba\xa7\xe5\x93\x81          </h3>\n          \t\t  <a href="/lists/index/products_storeId-81/" target="_blank" title="\xe6\x9b\xb4\xe5\xa4\x9a" class="more" ></a>\n                    <div class="widget1 widget-head"></div>\n        </div>\n        <div class="main_tright"></div>\n        <div class="clear"></div>\n      </div>\n      <div class="column_main cm_normal widget-content">\n        <div class="Loan_varieties">\r\n    <ul>\r\n    \t\t\t<li> \r\n\t\t\t<a href="/lists/info/products_storeId-81-infoId-358895" target="_blank" title="\xe6\x88\x90\xe9\x83\xbd\xe5\x85\x8d\xe6\x8a\xb5\xe6\x8a\xbc\xe5\x85\x8d\xe6\x8b\x85\xe4\xbf\x9d\xe8\xb4\xb7\xe6\xac\xbe" style="width:75px; height:74px; background:none; border:none; margin-right:15px; _margin-right:5px;"  value="591"> \r\